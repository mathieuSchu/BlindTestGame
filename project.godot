; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[ambientcg]

extract_path="res://AmbientCG/Downloads"
material_file_directory="res://AmbientCG/Materials"
environment_file_directory="res://AmbientCG/Environments"

[application]

config/name="BlindTestGame"
run/main_scene="res://Scene/main.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://Asset/icon.svg"

[autoload]

Global="*res://Sripts/global.gd"
SignalInt="*res://Sripts/signal_int.gd"

[display]

window/size/initial_position_type=4

[editor_plugins]

enabled=PackedStringArray("res://addons/ambientcg/plugin.cfg", "res://addons/kenyoni/pot_generation/plugin.cfg", "res://addons/qr_code/plugin.cfg")

[file_customization]

folder_colors={
"res://Asset/": "green",
"res://Question/": "orange",
"res://Scene/": "blue",
"res://Sripts/": "red",
"res://shader/": "pink"
}

[filesystem]

import/blender/enabled=false

[input]

end_manche={
"deadzone": 0.2,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":true,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":77,"key_label":0,"unicode":0,"location":0,"echo":false,"script":null)
]
}
end_game={
"deadzone": 0.2,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":true,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":71,"key_label":0,"unicode":0,"location":0,"echo":false,"script":null)
]
}

[plugins]

kenyoni/pot_generation/paths=Array[PackedStringArray]([])

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
